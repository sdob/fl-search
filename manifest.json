{
  "manifest_version": 2,
  "name": "Fallen London Item Search",
  "description": "This extension adds item and quality search to the 'Myself' tab in Fallen London.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://fallenlondon.storynexus.com/Gap/Load*"],
      "css": ["flis-styles.css"],
      "js": [
        "jquery-3.1.1.min.js",
        "mutation-summary.js",
        "flis-extension.js"
      ],
      "run_at": "document_end"
    }
  ]
}
